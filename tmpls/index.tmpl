<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title>Domains</title>
    <style>
    html, body{
        font-family: "Monaco", "Microsoft Yahei", Arial, "Helvetica Neue", Helvetica, "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Geneva, Verdana, sans-serif;
        padding: 20px 10px;
        font-weight: 100;
    }
    ul {
        list-style: none;
        padding: 0;
        margin: 0;
    }
    ul li {
        margin-right: 5px;
        line-height: 1.6em;
        display: inline-block;
        font-size: 12px;
    }
    h3 {
        margin: 2em 0;
    }
    h4 {
        margin: 10px 0;
    }
    </style>
</head>
<body>
    <h1>Domains</h1>
    <div class="home">
        <div>
            <h3>4 characters domains</h3>
            <div>
                <ul>
                    {{ range .Char4.Domains }}
                        <li>{{ . }}</li>
                    {{ end }}
                </ul>
            </div>
            {{ range .Char4.Keyword }}
                {{ if len .Domains }}
                <div>
                    <h4>{{ .Keyword }}</h4>
                    <ul>
                        {{ range .Domains }}
                        <li>{{ . }}</li>
                        {{ end }}
                    </ul>
                </div>
                {{ end }}
            {{ end }}
        </div>
        <div>
            <h3>5 characters domains</h3>
            <div>
                <ul>
                    {{ range .Char5.Domains }}
                        <li>{{ . }}</li>
                    {{ end }}
                </ul>
            </div>
            {{ range .Char5.Keyword }}
                {{ if len .Domains }}
                <div>
                    <h4>{{ .Keyword }}</h4>
                    <ul>
                        {{ range .Domains }}
                        <li>{{ . }}</li>
                        {{ end }}
                    </ul>
                </div>
                {{ end }}
            {{ end }}
        </div>
    </div>
</body>
</html>
